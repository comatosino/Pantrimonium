   
    <!-- content here -->
    <div class="container my-3 mx-auto">
      <div class="row col-12">
      {{#each items}}
        <div class="rounded col-md-4 col-sm-12">
          <div class="btn rounded mx-3 px-2 align-middle btn-block text-wrap" style="background-image: url('images/bgFridge.png');">
            <h2>{{name}}</h2>
          </div>
          <table class="rounded mx-3 px-2 table-hover" style="background-color: #ff9a4a;" width="100%">
          {{#each items}}
            <tr>
              <td width="40%">{{name}}</td>
              <td width="35%">{{quantity}} {{unit_of_measurement}}</td>
              <td width="25%" class="text-right">{{type}}</td>
            </tr>
          {{/each}}
          </Table>
          <!-- total -->
          <div class="btn rounded mx-3 px-2 my-0 text-right align-middle btn-block" style="background-color: #ffb202;">
              <h5>Total: {{items.length}}</h5>
          </div> 
        </div>
        <p>
      {{/each}}
      </div>  
    </div>
  
    <p class="p-4">  
    <footer class="pos-f-t no-gutters" >
        <nav class="navbar fixed-bottom justify-content-center" style="background-color: #ff8201;">
            <!-- Footer -->
        </nav>
    </footer>
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
</body>
</html>
