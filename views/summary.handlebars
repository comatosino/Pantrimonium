    <!-- content here -->

    <div class="col-11 mx-0 px-0 py-2 my-3 no-gutters w-100 mx-auto">
      <div class="row">

      {{#each items}}
        <div class="rounded col-md-3 mx-3">
          <div class="btn rounded mx-3 px-2 text-center align-middle btn-block text-wrap" style="background-image: url('images/bgFridge.png');">
            <h2>{{name}}</h2>
          </div>
          {{#each items}}
          <div class="rounded runningLow mx-3 my-0 p-2 text-center align-middle btn-block" style="background-color: #ff9a4a;">
            <span>{{name}}</span>
            <span>{{quantity}} {{unit_of_measurement}}</span>
            <span>{{type}}</span>
          </div>
          {{/each}}
          <!-- total -->
          <div class="btn rounded mx-3 px-2 my-0 text-center align-middle btn-block" style="background-color: #ffb202;">
              <h5>Total: {{items.length}}</h5>
          </div> 
        </div>
        <p>
      {{/each}}
      </div>  
    </div>
  
    <p class="p-4">  
    <footer class="pos-f-t no-gutters" >
        <nav class="navbar fixed-bottom justify-content-center" style="background-color: #ff8201;">
            <!-- Footer -->
        </nav>
    </footer>
